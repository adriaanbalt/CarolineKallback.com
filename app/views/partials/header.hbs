<div class="contain-to-grid fixed" data-move>
	<div class="direct-buy-bar">
		<nav data-topbar data-options="is_hover: false; back_text: <span class='icon-left-dir'></span> Back;"
				 class="top-bar sonos-bar">
			<section class="top-bar-section">
				<ul class="left">
					<li>
						<div class="row collapse">
							<div class="columns small-12">
								{{#if header.direct-buy.message}}
									<a id="#direct-buy-message" data-reveal-id="direct-buy-message" class="message white">{{ header.direct-buy.message }}</a>
								{{/if}}
							</div>
						</div>
					</li>
				</ul>
				<ul class="right show-for-large-up" data-move-from>
					<li class="has-dropdown" data-move-element>
						<a>{{ header.direct-buy.sales-help.link-title }}</a>
						<ul class="dropdown">
							<li class="label">
								<label>{{ header.direct-buy.sales-help.dropdown.speak-label }}</label>
							</li>
							<li><a
									href="http://www.sonos.com/{{ lang-locale }}/contact">{{ header.direct-buy.sales-help.dropdown.speak-call-link }}</a>
							</li>
							<li class="label">
								<label>{{ header.direct-buy.sales-help.dropdown.info-label }}</label>
							</li>
							<li><a
									href="http://www.sonos.com/{{ lang-locale }}/top-pre-purchase-questions">{{ header.direct-buy.sales-help.dropdown.info-questions-link }}</a>
							</li>
							<li>
								<a data-reveal-id="videoDemo">{{ header.direct-buy.sales-help.dropdown.info-video-demo }}</a>

								<div id="videoDemo" class="reveal-modal theater large" data-reveal data-background-color="dark">
									<div class="video-support-container">
										<div class="embed-single-video">
											<div data-tube data-video-id="{{ header.direct-buy.sales-help.dropdown.video-demo-id }}"></div>
										</div>
									</div>
									<a class="close-reveal-modal">&times;</a>
								</div>
							</li>
							<li class="label">
								<label>{{ header.direct-buy.sales-help.dropdown.order-label }}</label>
							</li>
							<li><a
									href="http://www.sonos.com/{{ lang-locale }}/support/policies">{{ header.direct-buy.sales-help.dropdown.order-returns-link }}</a>
							</li>
						</ul>
					</li>
				</ul>
			</section>
		</nav>
	</div>
	<nav data-topbar data-options="is_hover: false; back_text: <span class='icon-left-dir'></span> Back;"
			 class="top-bar sonos-bar">
		<ul class="title-area">
			<li class="name">
				{{#is lang-locale "en-us"}}
					<h1><a href="http://www.sonos.com">Sonos</a></h1>
				{{/is}}
				{{#isnt lang-locale "en-us"}}
					<h1><a href="http://www.sonos.com/{{ lang-locale }}">Sonos</a></h1>
				{{/isnt}}
			</li>
			<li class="toggle-topbar menu-icon"><a><span>Menu</span></a></li>
		</ul>
		<section class="top-bar-section">
			<ul class="left">
				<li class="has-dropdown">
					<a>{{ header.what-is-sonos.link-title }}</a>
					<ul class="dropdown">
						<li class="sep hide-for-medium-down"></li>
						<li><a href="http://www.sonos.com/{{ lang-locale }}/system">{{ header.what-is-sonos.dropdown.system }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/streaming-music">{{ header.what-is-sonos.dropdown.music }}</a>
						</li>
						<li><a href="http://www.sonos.com/{{ lang-locale }}/app">{{ header.what-is-sonos.dropdown.app }}</a></li>
						<li class="sep hide-for-medium-down"></li>
					</ul>
				</li>
				<li class="has-dropdown">
					<a>{{ header.product-overview.link-title }}</a>
					<ul class="dropdown">
						<li class="sep hide-for-medium-down"></li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/products/wireless-speakers">{{ header.product-overview.dropdown.speakers }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/products/wireless-home-theater">{{ header.product-overview.dropdown.home-theater }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/products/wireless-stereo-components">{{ header.product-overview.dropdown.components }}</a>
						</li>
						<li class="sep hide-for-medium-down"></li>
					</ul>
				</li>
				<li class="has-dropdown">
					<a>{{ header.shop.link-title }}</a>
					<ul class="dropdown">
						<li class="sep hide-for-medium-down"></li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop">{{ header.shop.dropdown.all-products-link }}</a>
						</li>
						<li class="label">
							<label>{{ header.shop.dropdown.speakers-label }}</label>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/play1">{{ header.shop.dropdown.play1 }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/play3">{{ header.shop.dropdown.play3 }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/play5">{{ header.shop.dropdown.play5 }}</a>
						</li>
						<li class="label">
							<label>{{ header.shop.dropdown.home-theater-label }}</label>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/playbar">{{ header.shop.dropdown.playbar }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/sub">{{ header.shop.dropdown.sub }}</a>
						</li>
						<li class="label">
							<label>{{ header.shop.dropdown.components-label }}</label>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/connectamp">{{ header.shop.dropdown.connect-amp }}</a>
						</li>
						<li><a
								href="http://www.sonos.com/{{ lang-locale }}/sonos-shop/products/connect">{{ header.shop.dropdown.connect }}</a>
						</li>
					</ul>
				</li>
				<li><a href="http://www.sonos.com/{{ lang-locale }}/support">{{ header.support }}</a></li>
			</ul>
			<ul class="right stack">
				<li data-move-to><a href="http://www.sonos.com/shop/shoppingcart.aspx"><span class="icon-basket"></span></a>
				</li>
				<li><a href="http://www.sonos.com/storelocator"><span class="icon-location"></span> {{ header.find-a-store }}
				</a></li>
				<li><a data-reveal-id="countryLanguage">{{ header.language }}</a></li>
				<li><a href="https://www.sonos.com/login/?ReturnUrl=/">{{ header.sign-in }}</a></li>
			</ul>
		</section>
	</nav>
</div>

